{
  "name": "angular-component-library",
  "version": "0.1.0",
  "description": "angular-component-library",
  "scripts": {
    "start:proxy": "ng serve --proxy-config proxy.conf.json",
    "start:mock": "ng serve --configuration=mock",
    "ng": "ng",
    "clean": "rimraf ./dist",
    "add_libs": "chapichapi-ng-libs add",
    "remove_libs": "chapichapi-ng-libs remove",
    "build_libs": "chapichapi-ng-libs build",
    "configs_libs": "chapichapi-ng-libs configs",
    "serve_libs": "chapichapi-ng-libs serve",
    "start": "npm run serve_libs",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test --watch=false --browsers=ChromeHeadlessNoSandbox",
    "test:watch": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:ci": "npm run clean && npm install && npm rebuild && npm run test && npm run build:prod",
    "deploy:github-pages": "ng deploy --base-href=/angular-component-library/ --name=\"rogue-elephant\" --email=jup.trandell@gmail.com",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\""
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~9.0.2",
    "@angular/cdk": "^9.1.3",
    "@angular/common": "~9.0.2",
    "@angular/compiler": "~9.0.2",
    "@angular/core": "~9.0.2",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "~9.0.2",
    "@angular/material": "^9.1.3",
    "@angular/platform-browser": "~9.0.2",
    "@angular/platform-browser-dynamic": "~9.0.2",
    "@angular/router": "~9.0.2",
    "@stackblitz/sdk": "^1.3.0",
    "bootstrap": "^4.0.0-alpha.6",
    "ngx-markdown": "^9.1.1",
    "ngx-timeago": "^1.0.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.3",
    "@angular-devkit/build-ng-packagr": "~0.901.0",
    "@angular/cli": "~9.0.3",
    "@angular/compiler-cli": "~9.0.2",
    "@angular/language-service": "~9.0.2",
    "@chapichapi/ngx-libs-helper": "0.0.8",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-ghpages": "^0.6.2",
    "codelyzer": "^5.1.2",
    "concurrently": "^5.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.16.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^10.1.1",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "shelljs": "^0.8.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5",
    "wait-on": "^5.0.0"
  }
}
